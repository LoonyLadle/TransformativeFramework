<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<Defs>
  <ThingDef ParentName="DrugPillBase">
    <defName>LuluTFFW_TestDrug</defName>
    <label>TFFW test drug</label>
    <description>Turns skin blue. That's it.</description>
    <graphicData>
      <texPath>Things/Item/Drug/Luciferium</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <rotatable>false</rotatable>
    <statBases>
      <MarketValue>120</MarketValue>
      <Mass>0.01</Mass>
    </statBases>
    <techLevel>Ultra</techLevel>
    <ingestible>
      <drugCategory>Medical</drugCategory>
      <outcomeDoers>
        <li Class="LoonyLadle.TFs.IngestionOutcomeDoer_InstantTFs">
          <transformation>LuluTFFW_TestTF</transformation>
        </li>
      </outcomeDoers>
    </ingestible>
    <comps>
      <li Class="CompProperties_Drug">
        <listOrder>1999</listOrder>
      </li>
    </comps>
    <tradeTags>
      <li>ExoticMisc</li>
    </tradeTags>
  </ThingDef>

  <LoonyLadle.TFs.TransformationDef>
    <defName>LuluTFFW_TestTF</defName>
    <label>TFFW test TF</label>
    <transformations>
      <li>
        <actions>
          <li Class="LoonyLadle.TFs.TFAct_SkinColor">
            <refName>TestSkin</refName>
            <colorGenerator Class="ColorGenerator_Single">
              <color>(0,0.5,1)</color>
            </colorGenerator>
          </li>
        </actions>
      </li>
    </transformations>
  </LoonyLadle.TFs.TransformationDef>
</Defs>
